{
  "name": "nestjs-2024",
  "version": "0.0.1",
  "description": "",
  "author": "uNique",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:dev": "nest build --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:create": "cross-var npm run typeorm -- migration:create ./src/database/migrations/$npm_config_name",
    "migration:generate": "cross-var npm run typeorm -- migration:generate ./src/database/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:run": "npm run typeorm -- migration:run",
    "prepare": "husky",
    "start:dev": "node --env-file ./environments/dev.env dist/src/main",
    "start:local": "nest start --watch -e \"node --env-file ./environments/local.env\"",
    "start:prod": "node --env-file ./environments/prod.env dist/src/main",
    "start:qa": "node --env-file ./environments/qa.env dist/src/main",
    "start:stage": "node --env-file ./environments/stage.env dist/src/main",
    "start:debug": "nest start --debug --watch -e \"node --env-file ./environments/local.env\"",
    "start:docker:dev": "docker compose --env-file ../nest-mini-project/environments/dev.env -f docker-compose.dev.yml up --build",
    "start:docker:local": "docker compose --env-file ./environments/local.env -f docker-compose.local.yml up --build",
    "start:docker:prod": "docker compose --env-file ./environments/prod.env -f docker-compose.prod.yml up --build",
    "start:docker:qa": "docker compose --env-file ./environments/qa.env -f docker-compose.qa.yml up --build",
    "start:docker:stage": "docker compose --env-file ./environments/stage.env -f docker-compose.stage.yml up --build",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --config ./jest-unit.json",
    "test:watch": "jest --watch",
    "typeorm": "typeorm-ts-node-commonjs --dataSource ./orm.config.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.688.0",
    "@nestjs/axios": "^3.1.2",
    "@nestjs/common": "^10.4.5",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.5",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.4.5",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "@sentry/nestjs": "^8.35.0",
    "@sentry/profiling-node": "^8.35.0",
    "bad-words": "^4.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-var": "^1.1.0",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/schematics": "^10.2.0",
    "@nestjs/testing": "^10.4.5",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "{src,apps,libs,test}/**/*.ts": "eslint --fix"
  }
}
